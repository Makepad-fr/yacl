{
  "name": "@makepad/yacl",
  "private": false,
  "version": "1.0.1",
  "description": "Yet an other cli library for JavaScript command line applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "prestart": "node_modules/.bin/tsc",
    "test": "node_modules/.bin/jasmine",
    "lint": "npm run fix",
    "prepublish": "node_modules/.bin/tsc",
    "preinstall": "node_modules/.bin/tsc",
    "precompile": "node_modules/.bin/gts fix",
    "compile": "npm run precompile && node_modules/.bin/tsc",
    "all-contributors": "node_modules/.bin/all-contributors",
    "generate-docs": "node_modules/.bin/typedoc --out docs src/",
    "clean": "node_modules/.bin/gts clean",
    "fix": "node_modules/.bin/gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "bin": {
    "yacl": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Makepad-fr/yacl.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "CLI",
    "Command",
    "line",
    "interface",
    "library",
    "argument",
    "parser",
    "tty"
  ],
  "author": "Makepad Developers <dev@makepad.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Makepad-fr/yacl/issues"
  },
  "homepage": "https://github.com/Makepad-fr/yacl#readme",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.13.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "firebase-tools": "^9.5.0",
    "gts": "^3.1.0",
    "jasmine": "^3.6.4",
    "jasmine-ts": "^0.1.2",
    "jsdoc": "^3.6.6",
    "typedoc": "^0.20.28",
    "typescript": "^4.0.3"
  }
}
